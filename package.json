{
  "name": "229-ledbury-chiropractic",
  "version": "1.0.0",
  "description": "Static site built with Eleventy and Laravel Mix for 229 Ledbury Chiropractic",
  "main": "index.js",
  "scripts": {
    "watch": "ELEVENTY_ENV=development npx @11ty/eleventy --config=eleventy.config.js --serve & webpack --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "debug": "DEBUG=Eleventy* ELEVENTY_ENV=development npx @11ty/eleventy --config=eleventy.config.js --serve & webpack --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "production": "NODE_ENV=production webpack --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js && ELEVENTY_ENV=production npx @11ty/eleventy --config=eleventy.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:granttransition/229-Ledbury-Chiropractic.git"
  },
  "author": "Grant Smith",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@quasibit/eleventy-plugin-sitemap": "^2.0.2",
    "browser-sync-webpack-plugin": "^2.0.1",
    "clean-css": "^4.2.3",
    "copy-webpack-plugin": "^6.2.1",
    "html-minifier": "^4.0.0",
    "imagemin-webpack-plugin": "^2.4.2",
    "laravel-mix": "^5.0.6",
    "laravel-mix-purgecss": "^4.2.0",
    "resolve-url-loader": "^3.1.0",
    "sass": "^1.27.0",
    "sass-loader": "^8.0.0",
    "terser": "^5.3.4",
    "vue-template-compiler": "^2.6.12"
  }
}
